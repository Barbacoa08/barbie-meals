{
  "name": "barbie-meals",
  "version": "0.1.0",
  "description": "A place for Barbie to store his favorite meals",
  "author": "barbajoe",
  "license": "MIT",
  "repository": "https://github.com/Barbacoa08/barbie-meals",
  "private": false,
  "proxy": "http://localhost:9000",
  "scripts": {
    "clean": "rm -rf coverage dist lambda-server",
    "lint": "eslint .",
    "build": "run-p build:**",
    "build:app": "react-scripts build",
    "build:lambda": "netlify-lambda build src/lambda-functions",
    "generate": "graphql-codegen --config codegen.js --require dotenv/config",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage=true --watchAll=false",
    "test:badges": "yarn test:coverage  && jest-coverage-badges --output ./badges",
    "storybook": "start-storybook -p 6006 -s public",
    "start": "run-p start:*",
    "start:app": "react-scripts start",
    "start:lambda": "netlify-lambda serve src/lambda-functions",
    "start-dev:lambda": "NODE_ENV=development netlify-lambda serve src/lambda-functions",
    "github": "yarn && yarn lint && yarn build && yarn test && yarn build-storybook",
    "prepublish": "yarn clean && yarn lint && yarn build && yarn test:badges"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*Types.ts",
      "!src/**/index.ts",
      "!src/**/*.stories.{jsx,tsx}",
      "!src/**/*.cypresstest.tsx"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@reach/router": "^1.3.4",
    "apollo-server-lambda": "2.25.2",
    "aws-lambda": "^1.0.7",
    "encoding": "^0.1.13",
    "framer-motion": "^4.1.17",
    "graphql": "^16.2.0",
    "netlify-lambda": "^2.0.15",
    "pouchdb": "^7.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5",
    "react-pathform": "^0.2.1",
    "react-scripts": "4.0.3",
    "reactn": "^2.2.7",
    "use-pouchdb": "^1.3.0",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.3.2",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.2",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.9",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/aws-lambda": "^8.10.89",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "@types/pouchdb": "^6.4.0",
    "@types/reach__router": "^1.3.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest-coverage-badges": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "pouchdb-adapter-memory": "^7.2.2",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
