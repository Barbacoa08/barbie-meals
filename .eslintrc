{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "standard",
    "plugin:jsx-a11y/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "prettier"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "legacyDecorators": true,
      "jsx": true
    }
  },
  "plugins": ["jsx-a11y", "@typescript-eslint"],
  "settings": {
    "react": {
      "version": "17"
    }
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}
